{
  "version": "0.0.0",
  "private": true,
  "name": "phonecat-typescript",
  "description": "A TypeScript conversion of Google's angular-phonecat library",
  "repository": "",
  "license": "MIT",
  "devDependencies": {
    "@types/angular": "^1.6.16",
    "@types/angular-resource": "^1.5.8",
    "@types/angularjs": "^1.5.14-alpha",
    "@types/node": "^7.0.15",
    "bower": "^1.5.2",
    "http-server": "^0.8.0",
    "karma": "^0.12.16",
    "karma-chrome-launcher": "^0.1.4",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jasmine": "~0.1.0",
    "protractor": "^2.1.0",
    "typescript": "^2.2.2"
  },
  "scripts": {
    "postinstall": "bower install && tsd install && npm run tsc",
    "tsc": "tsc -p app -out app/all.js",
    "prestart": "npm install",
    "start": "http-server -a 0.0.0.0 -p 8000",
    "pretest": "npm install",
    "test": "karma start test/karma.conf.js",
    "test-single-run": "karma start test/karma.conf.js  --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor test/protractor-conf.js"
  }
}
